    <hudson.tasks.Shell>
      <command>export BASE_GUS=$WORKSPACE

mkdir -p gus_home

source $BASE_GUS/setenv
# Something else should be making the project dirs!!!
mkdir -p $GUS_HOME/config/PlasmoDB;
mkdir -p $GUS_HOME/config/CryptoDB;
mkdir -p $GUS_HOME/lib/java/db_driver;

cp $PROJECT_HOME/install/gus.config.sample $GUS_HOME/config/gus.config
cp $ORACLE_HOME/jdbc/lib/ojdbc6.jar $GUS_HOME/lib/java/db_driver/

bld EuPathDBIrods

# There will be multiple calls here - a different yaml for each project
eupathSiteConfigure -model PlasmoDB -filename $BASE_GUS/plasmoDBMetaConfigure.yaml
eupathSiteConfigure -model CryptoDB -filename $BASE_GUS/cryptoDBMetaConfigure.yaml
</command>
